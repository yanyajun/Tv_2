<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="@dimen/h_14" />
            <corners android:radius="@dimen/h_3"/>
            <solid android:color="#00000000" />
            <stroke android:color="#a5ffffff" android:width="@dimen/h_3"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape><size android:height="@dimen/h_14" />
                <corners android:radius="@dimen/h_3"/>
                <solid android:color="#a5ffffff" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape><size android:height="@dimen/h_14"/>
                <corners android:radius="@dimen/h_3"/>
                <gradient
                    android:startColor="#94da6d"
                    android:endColor="#efdd0b"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>
</layer-list>